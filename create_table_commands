
create table customer(
  c_id number(6) primary key,
  name varchar(20),
  address varchar(50),
  email_address varchar(50)
  );
  --				email_address references credentials
  
create table agent(
  a_id number(6) primary key,
  name varchar(20), 
  email_address varchar(50)
  );		
  --        email_address references credentials
  
  
